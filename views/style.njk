<style>
	* {
		color:{{css.colors.font}};
		background:transparent;
		border:0;
		box-sizing:border-box;
		-moz-box-sizing:border-box;
		-webkit-box-sizing:border-box;
		-webkit-font-smoothing:antialiased;
		font-size:100%;
		font-weight:normal;
		margin:0;
		outline:0;
		padding:0;
		vertical-align:baseline;
		z-index:1;
	}

	body {
		background-color:{{css.colors.bodyBg}};
		font-family:Montserrat, Helvetica Neue, Helvetica, sans-serif;
		margin:0;
		min-width:55.625em; /* 890/16 */
		overflow-x:hidden;
	}

	body a {
		color:{{css.colors.link}};
		text-decoration:none;
		-webkit-transition:all 0.2s ease-in-out;
		transition:all 0.2s ease-in-out;
	}

	body a:hover {
		color:{{css.colors.link}};
		cursor:pointer;
		text-decoration:underline;
	}

	body b,
	body strong {
		font-weight:bold;
	}

	body input[type="password"],
	body input[type="number"],
	body input[type="email"],
	body input[type="text"],
	body textarea,
	body select {
		color:#000;
		background-color:#bbbbbb;
		border:0.0625em solid #dadada; /* 1/16 */
		border-radius:0;
		box-shadow: inset 0 0.0625em 0.25em {{css.colors.primary}}; /* 1/16 */ /* 4/16 */
		height:1.875em; /* 30/16 */
		padding:0.3125em; /* 5/16 */
	}

	body button,
	body button[type="submit"],
	body button[type="reset"],
	body input[type=button],
	body input[type=submit],
	body input[type=reset] {
		background-color:{{css.colors.secondary}};
		border-color:{{css.colors.secondary}};
		border-radius:0.1875em; /* 3/16 */
		color:{{css.colors.font}};
		cursor:pointer;
		height:1em; /* 16/16 */
		line-height:0;
		padding:1em 1.25em; /* 16/16 */ /* 20/16 */
		-webkit-transition:all 0.2s ease-in-out;
		transition:all 0.2s ease-in-out;
	}

	body button:hover,
	body button[type="submit"]:hover,
	body button[type="reset"]:hover,
	body input[type=button]:hover,
	body input[type=submit]:hover,
	body input[type=reset]:hover {
		background-color:{{css.colors.font}};
		color:#555;
	}

	body select option {
		color:#000;
	}

	body table {
		border-collapse:collapse;
		table-layout:fixed;
	}

	.clear {
		clear:both;
	}

	#header-image {
		background-image:url({{css.img.bg}});
	    background-position:top center;
		background-repeat:no-repeat;
	    background-size:cover;
	    height:100vh;
		left:0;
		min-width:55.625em; /* 890/16 */
		position:absolute;
		right:0;
	}

	#header-image:after {
		background:#1d1d1d;
		background:-webkit-gradient(linear, left top, left bottom, from(rgba(29,29,29,0.2)),to(rgba(29,29,29,1)));
		background:linear-gradient(to bottom, rgba(29,29,29,0.2) 0%,rgba(29,29,29,1) 100%);
		content: "";
		display: block;
		filter:progid:DXImageTransform.Microsoft.gradient( startColorstr='#1d1d1d', endColorstr='#1d1d1d',GradientType=0 );
		height:100vh;
	}

	.content-main {
		background-color:{{css.colors.bg}};
		font-family:Droid Serif, Georgia, serif;
		margin:0 auto;
		max-width:90%;
		min-height:12.5em; /* 200/16 */
		position:relative;
		top:3.75em; /* 60/16 */
		width:77.5em; /* 1240/16 */
	}

	.content-main blockquote {
		background-color:{{css.colors.primary}};
		border:1px dotted #999;
		font-style:italic;
		margin:2.142857142857143em 0; /* 30/14 */
		padding:0.7142857142857143em; /* 10/14 */
	}

	.content-blackout {
		background-color:rgba(0, 0, 0, 0.75);
		height:100%;
		position:fixed;
		opacity:0;
		top:0;
		-webkit-transition:all 0.2s ease-in-out;
		transition:all 0.2s ease-in-out;
		visibility:hidden;
		width:100%;
		z-index:2;
	}

	.content-blackout.active {
		opacity:1;
		visibility:initial;
	}

	.hidden {
		display:none !important;
	}

	.standard-form {
		overflow:hidden;
	}

	.standard-form label,
	.standard-form .standard-input {
		display:block;
		line-height:1.375em; /* 22/16 */
		min-height:1.25em; /* 20/16 */
		margin-top:0.0625em; /* 1/16 */
	}

	.standard-form label {
		background-color:{{css.colors.primary}};
		font-weight:bold;
		padding:1.25em; /* 20/16 */
	}

	.standard-form label[required]:after {
		content:" (required)";
	}

	.standard-form .standard-input {
		background-color:{{css.colors.header}};
		padding:0.9375em 1.25em; /* 15/16 */ /* 20/16 */
	}

	.standard-form .standard-input.error input {
		border:0.0625em solid #ff0000; /* 1/16 */
	}

	.standard-form .standard-input .input-error {
		background-color: #9D3834;
		border:0.0625em dashed #fff; /* 1/16 */
		display:none;
		padding:0.3125em 0.625em; /* 5/16 */ /* 10/16 */
		text-align:center;
	}

	.standard-form .standard-input.error .input-error {
		display:inline-block;
	}

	.standard-form .standard-input * {
		max-width:100%;
	}

	.standard-form input[type=submit] {
		float:right;
		margin:1.875em 0; /* 30/16 */
	}

	.multistep {
		overflow-x:hidden;
	}

	.multistep .header {
		background-color:{{css.colors.header}};
		margin-bottom:1px;
		padding:8px 20px;
	}

	.multistep .steps {
		display:flex;
		left:0px;
		position:relative;
		transition:left 0.5s ease-in-out;
	}

	.multistep .steps .step {
		display:none;
		flex-basis:100%;
		flex-shrink:0;
	}

	.multistep .steps .step.active {
		display:initial;
	}

	.multistep .nav {
		background-color:{{css.colors.header}};
	}

	.multistep .nav .previous,
	.multistep .nav .next,
	.multistep .nav .submit {
		display:inline-block;
		padding:6px 20px;
		transition:all 0.2s ease-in-out;
	}

	.multistep .nav .previous:hover:not(.disabled),
	.multistep .nav .next:hover:not(.disabled),
	.multistep .nav .submit:hover {
		background-color:{{css.colors.primary}};
		cursor:pointer;
	}

	.multistep .nav .previous.disabled,
	.multistep .nav .next.disabled {
		color:#888;
	}

	.multistep .nav .previous {
		border-right:1px solid {{css.colors.bg}};
	}

	.multistep .nav .next,
	.multistep .nav .submit {
		border-left:1px solid {{css.colors.bg}};
		float:right;
	}

	.multistep .nav .submit {
		padding:8px 20px;
	}

	.multistep .nav .error {
		background-color: #9D3834;
		border:0.0625em dashed #fff; /* 1/16 */
		display:none;
		float:right;
		margin:0.14285714285714285em 0.35714285714285715em; /* 2/14 */ /* 5/14 */
		padding:0.3125em 0.625em; /* 5/16 */ /* 10/16 */
		text-align:center;
	}

	.multistep .nav .error.active {
		display:inline-block;
	}

	.radio .option:not(.disabled) label:hover {
		cursor:pointer;
	}

	.radio .option.disabled label {
		color:#888;
	}

	.new-post {
		border:0.0625em solid #585858; /* 1/16 */
		margin-top:0.9375em; /* 15/16 */
		padding:1.5625em; /* 25/16 */
	}

	.new-post legend {
		background-color:{{css.colors.primary}};
		display:block;
		font-size:0.875em; /* 14/16 */
		padding:0.35714285714285715em; /* 5/14 */
	}

	.new-post label {
		font-size:0.875em; /* 14/16 */
	}

	.new-post form input {
		display:block;
		margin-bottom:0.5em; /* 8/16 */
		width:100%;
	}

	.new-post form input[type=submit] {
		display:inline-block;
		float:right;
		margin-top:0.5em; /* 8/16 */
		width:auto;
	}

	.warning {
		background-color:#ffffe0;
		border:0.07142857142857142em solid #e6db55; /* 1/14 */
		border-radius:0.21428571428571427em; /* 3/14 */
		color:#856404;
		font-size:0.875em; /* 14/16 */
		padding:0.7142857142857143em; /* 10/14 */
	}

	.role {
		text-transform:capitalize;
	}

	.tinymce {
		height:12.5em; /* 200/16 */
	}

	.page {
		display:flex;
		padding:1.875em; /* 30/16 */
	}

	.page.full {
		display:block;
	}

	.noselect {
		-webkit-user-select:none;
		-moz-user-select:none;
		-ms-user-select:none;
		user-select:none;
	}
</style>
